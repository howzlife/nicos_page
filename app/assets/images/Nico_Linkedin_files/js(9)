(function(){dust.register("tl/apps/profile/my_profile/partial/select_with_value",j);
function j(n,m){return n.write('<select name="').reference(m.getPath(true,["name"]),m,"h").write('" id="').reference(m.getPath(true,["id"]),m,"h").write('" type="').reference(m.getPath(true,["type"]),m,"h").write('" aria-describedby="').exists(m.get("errMsgId"),m,{"else":i,"block":h},null).write('">').exists(m.get("initialOption"),m,{"block":g},null).exists(m.get("selectedValue"),m,{"else":f,"block":c},null).write("</select>")
}function i(n,m){return n.reference(m.getPath(true,["id"]),m,"h").write("-error")
}function h(n,m){return n.reference(m.get("errMsgId"),m,"h")
}function g(n,m){return n.write('<option selected="" value="">').reference(m.get("initialOption"),m,"h").write("</option>")
}function f(n,m){return n.section(m.get("options"),m,{"block":e},null)
}function e(n,m){return n.write('<option value="').reference(m.getPath(true,["value"]),m,"h").write('" ').exists(m.getPath(true,["selected"]),m,{"block":d},null).write(">").reference(m.getPath(true,["label"]),m,"h").write("</option>")
}function d(n,m){return n.write('selected="selected"')
}function c(n,m){return n.section(m.get("options"),m,{"block":b},null)
}function b(n,m){return n.write('<option value="').reference(m.getPath(true,["value"]),m,"h").write('" ').helper("eq",m,{"block":a},{"key":l,"value":k}).write(">").reference(m.getPath(true,["label"]),m,"h").write("</option>")
}function a(n,m){return n.write('selected="selected"')
}function l(n,m){return n.reference(m.getPath(true,["value"]),m,"h")
}function k(n,m){return n.reference(m.get("selectedValue"),m,"h")
}return j
})();
